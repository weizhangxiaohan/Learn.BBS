
@{
    ViewBag.Title = "List";
}
@using learn.bbs.dal.EfModel;
@model IList<bbs_post>

<div class="container">
    @{
        foreach (bbs_post post in Model)
        {
            <div class="row">
                <div class="col-lg-12">@post.title</div>
            </div>
        }
    }

</div>


