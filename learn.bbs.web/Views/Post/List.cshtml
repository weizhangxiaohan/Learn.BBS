
@{
    ViewBag.Title = "List";
}
@using learn.bbs.dal.EfModel;
@model IList<bbs_post>

<div class="container">
    <div class="row">
        <div class="col-lg-4 col-lg-offset-8"><a href="/post/@ViewBag.AreaUid/add">我要发帖</a></div>
    </div>
    @{
        foreach (bbs_post post in Model)
        {
            <div class="row" style="padding:5px">
                <div class="col-lg-12"><a href="#">@post.title</a></div>
            </div>
        }
    }
</div>


